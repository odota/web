{
  "parserOptions": {
    "ecmaFeatures": { "experimentalObjectRestSpread": true }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolver": "webpack"
  },
  "extends": "airbnb",
  "rules": {
    "max-len": ["error", 150],
    "no-shadow": 0,
    "react/prop-types": 0,
    "import/named": 2,
    "import/no-dynamic-require": 0,
    "jsx-a11y/no-static-element-interactions": 0
  }
}
