{
  "name": "opendota-ui",
  "description": "Dota 2 Data Web UI",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "webpack": "NODE_ENV=production webpack",
    "start": "webpack-dev-server --hot --inline --compress",
    "start:noc": "webpack-dev-server --hot --inline",
    "build": "npm install && npm run lintnofix && npm run lint:css && npm run webpack",
    "lint": "eslint . --ext .js,.jsx --fix",
    "lintnofix": "eslint . --ext .js,.jsx",
    "lint:css": "stylelint 'src/**/*.css'",
    "test": "NODE_PATH=./src NODE_ENV=test mocha --ui bdd --reporter nyan -r test/initializeJsdom.js $(find src -name '*.spec.*')",
    "test:watch": "npm test -- --watch --watch-extensions jsx,js"
  },
  "greenkeeper": {
    "label": "type: greenkeeper"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/odota/ui"
  },
  "dependencies": {
    "abcolor": "^0.5.5",
    "c3": "^0.4.12",
    "classnames": "^2.2.5",
    "dotaconstants": "^5.8.0",
