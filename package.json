    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.7.0",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.5",
    "dirty-chai": "^1.2.2",
    "enzyme": "2.8.2",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^15.0.1",
