    "@types/nanoid": "^2.1.0",
    "@types/node": "^24.5.2",
    "@types/papaparse": "^5.0.3",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.40",
    "@types/react-content-loader": "^3.1.4",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.11",
